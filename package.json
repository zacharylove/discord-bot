  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^1.6.5",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/rest": "^2.2.0",
    "@discordjs/voice": "^0.17.0",
    "axios": "^1.6.8",
    "canvas": "^2.11.2",
    "concurrently": "^8.2.2",
    "discord-api-types": "^0.37.83",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "ffmpeg-static": "^5.2.0",
    "fs-capacitor": "^8.0.0",
    "get-youtube-id": "^1.0.1",
    "gifuct-js": "^2.1.2",
    "iso8601-duration": "^2.1.2",
    "libsodium": "^0.7.13",
    "libsodium-wrappers": "^0.7.13",
    "module-alias": "^2.2.3",
    "mongoose": "^8.3.2",
    "p-retry": "^6.2.0",
    "play-dl": "^1.9.7",
    "spotify-uri": "^4.0.1",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^20.14.0",
    "@types/spotify-web-api-node": "^5.0.11",
    "fluent-ffmpeg": "^2.1.2",
